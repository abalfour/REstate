{
  "version": "0.1.0-*",
  "authors": [
    "Donovan (Ovan) Crone",
    "Andrea Balfour"
  ],
  "description": "REstate: a StateMachine-As-A-Service concept.",
  "packOptions": {
    "requireLicenseAcceptance": false,
    "licenseUrl": "https://opensource.org/licenses/MIT",
    "projectUrl": "",
    "tags": [
      "StateMachine",
      "SMAAS",
      "SM-AAS"
    ],
    "owners": [
      "Donovan (Ovan) Crone",
      "Andrea Balfour"
    ],
    "summary": "REstate: a StateMachine-As-A-Service concept."
  },
  "buildOptions": {
    "emitEntryPoint": true,
    "debugType": "portable",
    "copyToOutput": {
      "includeFiles": [
        "appconfig.json",
        "REstate.pfx",
        "web.config"
      ],
      "include": "Views/**"
    }
  },
  "publishOptions": {
    "includeFiles": [
      "appconfig.json",
      "REstate.pfx",
      "web.config"
    ]
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.1",
          "type": "platform"
        }
      },
      "imports": "portable-net45+win8+wp8+wpa81"
    }
  },
  "dependencies": {
    "Serilog": "2.3.0",
    "Serilog.Sinks.Logentries": "3.0.0-dev-00020",
    "Microsoft.AspNetCore.Hosting": "1.0.0",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.1",
    "Microsoft.AspNetCore.Owin": "1.0.0",
    "Nancy": "2.0.0-barneyrubble",
    "Nancy.Authentication.Stateless": "2.0.0-barneyrubble",
    "Autofac": "4.1.1",
    "REstate.Common": "0.1.0-*",
    "REstate.Engine": "0.1.0-*",
    "Nancy.Bootstrappers.Autofac": "2.0.0-Pre458",
    "Microsoft.Extensions.Configuration.Json": "1.1.0",
    "Microsoft.Extensions.Configuration.Binder": "1.0.0",
    "REstate.Scheduler": "0.1.0-*",
    "REstate.Scheduler.Consumer.Direct": "0.1.0-*",
    "REstate.Engine.Repositories.MSSQL": "0.1.0-*",
    "REstate.Scheduler.Repositories.MSSQL": "0.1.0-*",
    "REstate.Connectors.Scheduler.Direct": "0.1.0-*",
    "REstate.Auth": "0.1.0-*",
    "REstate.Auth.Repositories.MSSQL": "0.1.0-*",
    "JwtAndCookie": "2.0.0-*",
    "Microsoft.AspNetCore.Diagnostics": "1.0.0",
    "Serilog.Sinks.Literate": "2.0.0",
    "REstate.Connectors.RabbitMQ": {
      "target": "project",
      "version": "0.1.0-*"
    },
    "Microsoft.AspNetCore.AzureAppServicesIntegration": "1.0.0-preview1-final"
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": {
      "version": "1.0.0-preview2-final",
      "imports": "portable-net45+win8+dnxcore50"
    }
  },
  "runtimes": {
    "win10-x64": {}
  },
  "scripts": {
    "postpublish": [
      "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
    ]
  }
}